{
  "moduleId": "update-module",
  "moduleName": "更新模块",
  "moduleDescription": "提供应用更新功能的模块化规则",
  "moduleType": "feature",
  "priority": 40,
  "dependencies": {
    "required": ["logging"],
    "optional": ["network"]
  },
  "parameters": {
    "MODULE_NAME": {
      "description": "模块名称（如：AppUpdate、SystemUpdate）",
      "required": true,
      "default": "AppUpdate",
      "prompt": "请输入模块名称"
    },
    "MODULE_PATH": {
      "description": "模块代码路径（如：lib/update、src/update）",
      "required": true,
      "default": "lib/update",
      "prompt": "请输入模块路径"
    },
    "UPDATE_SERVER_URL": {
      "description": "更新服务器地址",
      "required": false,
      "default": "",
      "prompt": "请输入更新服务器地址（可选）"
    }
  },
  "compatibleLanguages": ["dart", "typescript", "python", "kotlin"],
  "compatibleFrameworks": ["flutter", "react", "django", "android"]
}


