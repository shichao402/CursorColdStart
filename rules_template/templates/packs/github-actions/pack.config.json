{
  "id": "github-actions",
  "name": "GitHub Actions CI/CD",
  "description": "GitHub Actions 工作流规范，包括两阶段发布流程、自动构建号递增、更新配置生成",
  "category": "toolchain",
  "priority": 45,
  "dependencies": ["version-management"],
  "config": {
    "buildTagPrefix": {
      "type": "string",
      "default": "build",
      "description": "构建标签前缀"
    },
    "releaseTagPrefix": {
      "type": "string",
      "default": "v",
      "description": "发布标签前缀"
    }
  }
}
